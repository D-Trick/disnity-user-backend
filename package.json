{
    "name": "disnity-user-backend",
    "version": "4.5.0",
    "description": "Disnity User Backend",
    "author": "Trick",
    "license": "UNLICENSED",
    "private": true,
    "repository": "https://github.com/D-Trick/disnity-user-backend",
    "scripts": {
        "start": "NODE_ENV=production nest start",
        "start:dev": "rm -rf ./dist && NODE_ENV=development nest start --watch",
        "start:dev:pm2": "NODE_ENV=development pm2 start pm2.config.js",
        "start:prod": "NODE_ENV=production pm2 start pm2.config.js",
        "start:debug": "NODE_ENV=development nest start --debug --watch",
        "build": "nest build",
        "prebuild": "rimraf dist",
        "reload:dev": "NODE_ENV=development pm2 reload pm2.config.js",
        "reload": "NODE_ENV=production pm2 reload pm2.config.js",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test:clear": "jest --clearCache",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:unit": "NODE_ENV=test jest --config ./test/jest-unit.json",
        "test:unit:watch": "NODE_ENV=test jest --watch --config ./test/jest-unit.json",
        "test:unit:cov": "NODE_ENV=test jest --coverage --config ./test/jest-unit.json",
        "test:integration": "NODE_ENV=test jest --config ./test/jest-integration.json",
        "test:integration:watch": "NODE_ENV=test jest --watch --config ./test/jest-integration.json",
        "test:integration:cov": "NODE_ENV=test jest --coverage --config ./test/jest-integration.json",
        "test:e2e": "NODE_ENV=test jest --detectOpenHandles --forceExit --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs/axios": "3.0.2",
        "@nestjs/cache-manager": "2.2.2",
        "@nestjs/common": "10.3.5",
        "@nestjs/core": "10.3.5",
        "@nestjs/jwt": "10.2.0",
        "@nestjs/mapped-types": "2.0.5",
        "@nestjs/microservices": "10.3.5",
        "@nestjs/passport": "10.0.3",
        "@nestjs/platform-express": "10.3.5",
        "@nestjs/typeorm": "10.0.2",
        "axios": "1.6.8",
        "cache-manager": "5.4.0",
        "cache-manager-redis-yet": "4.2.0",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "cookie-parser": "1.4.6",
        "dayjs": "1.11.10",
        "dotenv": "16.4.5",
        "ioredis": "5.3.2",
        "lodash": "4.17.21",
        "mysql2": "3.9.3",
        "nest-winston": "1.9.4",
        "passport": "0.7.0",
        "passport-jwt": "4.0.1",
        "passport-oauth2": "1.8.0",
        "reflect-metadata": "0.2.1",
        "request-ip": "3.3.0",
        "rimraf": "5.0.5",
        "rxjs": "7.8.1",
        "typeorm": "0.3.20",
        "winston": "3.13.0",
        "winston-daily-rotate-file": "5.0.0"
    },
    "devDependencies": {
        "@nestjs/cli": "10.3.2",
        "@nestjs/schematics": "10.1.1",
        "@nestjs/testing": "10.3.5",
        "@types/cache-manager": "4.0.6",
        "@types/cookie-parser": "1.4.7",
        "@types/express": "4.17.21",
        "@types/jest": "29.5.12",
        "@types/lodash": "4.17.0",
        "@types/node": "20.11.30",
        "@types/passport-jwt": "4.0.1",
        "@types/passport-oauth2": "1.4.15",
        "@types/request-ip": "0.0.41",
        "@types/supertest": "6.0.2",
        "@typescript-eslint/eslint-plugin": "7.4.0",
        "@typescript-eslint/parser": "7.4.0",
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "jest": "29.7.0",
        "prettier": "3.2.5",
        "source-map-support": "0.5.21",
        "supertest": "6.3.4",
        "ts-jest": "29.1.2",
        "ts-loader": "9.5.1",
        "ts-mockito": "^2.6.1",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.4.3"
    }
}
