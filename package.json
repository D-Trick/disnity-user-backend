{
    "name": "disnity-user-backend",
    "version": "4.2.13",
    "description": "Disnity User Backend",
    "author": "Trick",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "start": "NODE_ENV=production nest start",
        "start:dev": "rm -rf ./dist && NODE_ENV=development nest start --watch",
        "start:dev:pm2": "NODE_ENV=development pm2 start pm2.config.js",
        "start:prod": "NODE_ENV=production pm2 start pm2.config.js",
        "start:debug": "NODE_ENV=development nest start --debug --watch",
        "build": "nest build",
        "prebuild": "rimraf dist",
        "reload:dev": "NODE_ENV=development pm2 reload pm2.config.js",
        "reload": "NODE_ENV=production pm2 reload pm2.config.js",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:unit": "NODE_ENV=test jest --config ./test/jest-unit.json",
        "test:unit:watch": "NODE_ENV=test jest --watch --config ./test/jest-unit.json",
        "test:unit:cov": "NODE_ENV=test jest --coverage --config ./test/jest-unit.json",
        "test:integration": "NODE_ENV=test jest --config ./test/jest-integration.json",
        "test:integration:watch": "NODE_ENV=test jest --watch --config ./test/jest-integration.json",
        "test:integration:cov": "NODE_ENV=test jest --coverage --config ./test/jest-integration.json",
        "test:e2e": "NODE_ENV=test jest --detectOpenHandles --forceExit --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs/axios": "3.0.1",
        "@nestjs/cache-manager": "2.1.1",
        "@nestjs/common": "10.2.8",
        "@nestjs/core": "10.2.8",
        "@nestjs/jwt": "10.1.1",
        "@nestjs/mapped-types": "2.0.2",
        "@nestjs/microservices": "10.2.8",
        "@nestjs/passport": "10.0.2",
        "@nestjs/platform-express": "10.2.8",
        "@nestjs/typeorm": "10.0.0",
        "axios": "1.6.0",
        "cache-manager": "5.2.4",
        "cache-manager-redis-yet": "4.1.2",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.0",
        "cookie-parser": "1.4.6",
        "dayjs": "1.11.10",
        "dotenv": "16.3.1",
        "ioredis": "5.3.2",
        "lodash": "4.17.21",
        "mysql2": "3.6.3",
        "nest-winston": "1.9.4",
        "passport": "0.6.0",
        "passport-jwt": "4.0.1",
        "passport-oauth2": "1.7.0",
        "reflect-metadata": "0.1.13",
        "request-ip": "3.3.0",
        "rimraf": "5.0.5",
        "rxjs": "7.8.1",
        "typeorm": "0.3.17",
        "winston": "3.11.0",
        "winston-daily-rotate-file": "4.7.1"
    },
    "devDependencies": {
        "@nestjs/cli": "10.2.1",
        "@nestjs/schematics": "10.0.3",
        "@nestjs/testing": "10.2.8",
        "@types/cache-manager": "4.0.4",
        "@types/cookie-parser": "1.4.5",
        "@types/express": "4.17.20",
        "@types/jest": "29.5.7",
        "@types/lodash": "4.14.200",
        "@types/node": "20.8.10",
        "@types/passport-jwt": "3.0.12",
        "@types/passport-oauth2": "1.4.14",
        "@types/request-ip": "0.0.40",
        "@types/supertest": "2.0.15",
        "@typescript-eslint/eslint-plugin": "6.9.1",
        "@typescript-eslint/parser": "6.9.1",
        "eslint": "8.53.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-prettier": "5.0.1",
        "jest": "29.7.0",
        "prettier": "3.0.3",
        "source-map-support": "0.5.21",
        "supertest": "6.3.3",
        "ts-jest": "29.1.1",
        "ts-loader": "9.5.0",
        "ts-mockito": "^2.6.1",
        "ts-node": "10.9.1",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.2.2"
    }
}
